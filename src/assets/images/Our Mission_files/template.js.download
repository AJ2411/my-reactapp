            var emptyInputs = $('.hrh-fieldset .input-required').find('input, select, textarea').filter(function() {
                return this.value === "";
            });
            if (!emptyInputs.length) {
                $form.addClass('active');
                if ($form.hasClass('active')) {
                    sendMessageBooking($form);
                    $('.hrh-widget-reservation .forms-style .message-error').removeClass('message-error');
                    $('.btn-send').parent().addClass('btn-no-active');
                    $form.removeClass('active');
                }
            } else {
                $('.hrh-fieldset .input-required').each(function() {
                    var $that = $(this),
